@article{nagakura_non-thermal_2021,
	title = {Non-thermal neutrinos created by shock acceleration in successful and failed core-collapse supernova},
	volume = {502},
	issn = {0035-8711, 1365-2966},
	url = {http://arxiv.org/abs/2010.15136},
	doi = {10.1093/mnras/stab040},
	abstract = {We present a comprehensive study of neutrino shock acceleration in core-collapse supernova (CCSN). The leading players are heavy leptonic neutrinos, \${\textbackslash}nu\_\{{\textbackslash}mu\}\$ and \${\textbackslash}nu\_\{{\textbackslash}tau\}\$; the former and latter potentially gain the energy up to \${\textbackslash}sim 100\$ MeV and \${\textbackslash}sim 200\$ MeV, respectively, through the shock acceleration. Demonstrating the neutrino shock acceleration by Monte Carlo neutrino transport, we make a statement that it commonly occurs in the early post bounce phase (\${\textbackslash}lesssim 50\$ ms after bounce) for all massive stellar collapse experiencing nuclear bounce and would reoccur in the late phase (\${\textbackslash}gtrsim 100\$ ms) for failed CCSNe. This opens up a new possibility to detect high energy neutrinos by terrestrial detectors from Galactic CCSNe; hence, we estimate the event counts for Hyper(Super)-Kamiokande, DUNE, and JUNO. We find that the event count with the energy of \${\textbackslash}gtrsim 80\$ MeV is a few orders of magnitude higher than that of the thermal neutrinos regardless of the detectors, and muon production may also happen in these detectors by \${\textbackslash}nu\_\{{\textbackslash}mu\}\$ with the energy of \${\textbackslash}gtrsim 100\$ MeV. The neutrino signals provide a precious information on deciphering the inner dynamics of CCSN and placing a constraint on the physics of neutrino oscillation; indeed, the detection of the high energy neutrinos through charged current reaction channels will be a smoking gun evidence of neutrino flavor conversion.},
	number = {1},
	urldate = {2022-04-14},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Nagakura, Hiroki and Hotokezaka, Kenta},
	month = jan,
	year = {2021},
	note = {arXiv: 2010.15136},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, High Energy Physics - Experiment, High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {89--107},
	annote = {Comment: Accepted to MNRAS},
	file = {arXiv Fulltext PDF:C\:\\Users\\p_h_m\\Zotero\\storage\\AVIWS3R4\\Nagakura and Hotokezaka - 2021 - Non-thermal neutrinos created by shock acceleratio.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\p_h_m\\Zotero\\storage\\TQNUV7C8\\2010.html:text/html},
}

