  keywords = {Astrophysics - High Energy Astrophysical Phenomena,High Energy Physics - Experiment,High Energy Physics - Phenomenology,Nuclear Experiment,Nuclear Theory},
  note = {Comment: Accepted to MNRAS},
  file = {C\:\\Users\\Pruthvi\\Zotero\\storage\\AVIWS3R4\\Nagakura and Hotokezaka - 2021 - Non-thermal neutrinos created by shock acceleratio.pdf;C\:\\Users\\Pruthvi\\Zotero\\storage\\TQNUV7C8\\2010.html}
}

